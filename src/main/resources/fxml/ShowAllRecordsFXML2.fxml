<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112"
            xmlns:fx="http://javafx.com/fxml/1">
    <bottom>
        <Button id="back" mnemonicParsing="false" text="Back" BorderPane.alignment="BOTTOM_CENTER"/>
    </bottom>
    <bottom>
        <Button id="refresh" mnemonicParsing="false" text="Refresh" BorderPane.alignment="TOP_LEFT"/>
    </bottom>
    <center>
        <TableView id="users statistic" prefHeight="350.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn id="logins" prefWidth="300" text="logins">
                    <cellValueFactory>
                        <PropertyValueFactory property="logins"/>
                    </cellValueFactory>
                </TableColumn>
                <TableColumn id="points" prefWidth="300" text="points">
                    <cellValueFactory>
                        <PropertyValueFactory property="points"/>
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
    </center>
</BorderPane>
